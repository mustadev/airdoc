{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Doctor set",
    "type": "array",
    "items": {
        "title": "Doctor",
        "type": "object",
        "properties": {
            "id": {
                "description": "The unique identifier for a doctor",
                "type": "string"
            },
            "lastName": {
                "type": "string"
            },
            "firstName": {
                "type": "string"
            },
            "password": {
                "type": "string"
            },
            "email": {
                "type": "string"
            },
            "speciality": {
                "type": "string"
            },
            "roles":{
            	"type": "array",
            	"items":{
            		"type":"string"
            	}
            },
            "services":{
            	"type": "array",
            	"items":{
            		"type":"string"
            	}
            },
	    	"description": {
                "type": "string"
            },
	    	"city": {
                "type": "string"
            },
            "rating": {
                "type": "number",
                "minimum": 0,
                "exclusiveMinimum": true
            },
            "reviews": {
                "type": "array",
                "items": {
                    "type": "object",
		    "properties": {
		        "content": {"type": "string"},
		        "auther": {"type": "object"},
		        "likes": {"type": "number"}
		    }
                }
            },
            "ownerId": {
                "type": "string"
                }

        },
        "required": ["id", "name", "ownerId"]
    }
}
